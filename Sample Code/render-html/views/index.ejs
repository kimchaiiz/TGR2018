<!DOCTYPE html>
<html>

<head>
  <% include head.ejs %>
</head>

<body>
  <header>
    <% include header.ejs %>
  </header>
  <main>
    <div class="jumbotron">
      <h1><%= title %></h1>
      <p>Welcome to templating using EJS</p>
    </div>
    <h2>Loop</h2>

    <ul>
      <% users.forEach(function(user) { %>
        <li>
          <%= user.name %> -
            <%= user.id %>
        </li>
        <% }); %>
    </ul>
  </main>
  <footer>
    <% include footer.ejs %>
  </footer>
</body>

</html>
